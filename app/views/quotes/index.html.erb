<div class="container
mx-auto
h-auto
text-cyan-50
text-center
pt-10
mt-3
shadow-lg
shadow-blue-700/50
rounded-lg
border-2
border-blue-700">
    <h1 class="font-bold text-5xl">Заметки </h1>
    <div class="flex justify-center pb-3">
        <%= link_to new_quote_path, data: { turbo_frame: dom_id(Quote.new) } do %>
            <%= render inline: Rails.root.join('public/text.svg').read %>
        <% end %>
    </div>
    <div>
        <%= turbo_frame_tag Quote.new %>

        <%= turbo_frame_tag "quotes" do %>
            <%= render @quotes %>
        <% end %>
    </div>
</div>
